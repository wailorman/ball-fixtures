{
  "name": "ball-fixtures",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/wailorman/ball-fixtures.git",
  "author": "Sergey Popov <wailorman@yandex.ru>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "dotenv-safe": "^5.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^22.4.3",
    "jest-environment-node-debug": "^2.0.0",
    "pg": "^7.4.1",
    "sequelize-cli": "^4.0.0",
    "sequelize": "^4.37.6"
  },
  "peerDependencies": {
    "sequelize": "^4.37.6"
  },
  "scripts": {
    "jest-debug": "NODE_ENV=test node --inspect ./node_modules/.bin/jest -i --runInBand --env jest-environment-node-debug --",
    "jest": "NODE_ENV=test node ./node_modules/.bin/jest --bail --expand -i --runInBand "
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.jsx?$",
    "collectCoverageFrom": [
      "{server}/schema/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/test-setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "server/.+"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  }
}

{
  "name": "ball-fixtures",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/wailorman/ball-fixtures.git",
  "author": "Sergey Popov <wailorman@yandex.ru>",
  "license": "MIT",
  "dependencies": {
    "lodash.pad": "^4.5.1"
  },
  "devDependencies": {
    "babel-jest": "^22.4.3",
    "chai": "^4.1.2",
    "dotenv-safe": "^5.0.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^22.4.3",
    "jest-environment-node-debug": "^2.0.0",
    "lodash": "^4.17.5",
    "pg": "^7.4.1",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0"
  },
  "peerDependencies": {
    "sequelize": "^4.37.6"
  },
  "scripts": {
    "jest-debug": "NODE_ENV=test node --inspect ./node_modules/.bin/jest -i --runInBand --env jest-environment-node-debug --",
    "jest": "NODE_ENV=test node ./node_modules/.bin/jest --bail --expand -i --runInBand "
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test))\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "server/.+"
    ]
  }
}

{
  "extends": "tslint-config-airbnb",
  "rules": {
    "import-name": ["off"],
    "align": false,
    "quotemark": false,
    "ter-arrow-parens": false
  },
  "globals": {
    "it": true,
    "describe": true,
    "before": true,
    "after": true,
    "beforeEach": true,
    "afterEach": true,
    "beforeAll": true,
    "afterAll": true
  }
}
